wait(0.7)
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("トイレの花子さん", "BloodTheme")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Hubs")

Section:NewButton("Kavo", "The Mimic", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/hanakokunsenpai/CTStudios-The-Mimic/main/thefourbeastscontroljealousyrebirthrage'))()
end)

